  field(PREC, "3")
  field(VAL,  "1")
  field(ASG, "KLYS_MAINT")
  field(FLNK, "$(PRIMARY):$(MICRO):$(UNIT):RE_RTM_SCALE")
  info(autosaveFields, "VAL")
}

